<launch>
   
    <!-- IMU -->
    <node pkg="myahrs_driver" type="myahrs_driver" name="myahrs_driver">
        <param name="port" value="/dev/ttyACM0" />
        <param name="baud_rate" value="115200" />
    </node>

    <!-- ARDUINO -->
    <!--  * encoders and rc inputs -->
    <node pkg="rosserial_python" type="serial_node.py" name="arduino_node" >
        <param name="port" value="/dev/ttyUSB0" />
    </node>

    <!-- ARBITRATION -->
    <node pkg="barc" type="controller_rc_demo_gui.py" name="arbitrator_node" output="screen" />

    <!-- GUI INPUTS-->
    <node pkg="zarc_gui" type="ECU_command.py" name="gui_node" output="screen" />

</launch>
